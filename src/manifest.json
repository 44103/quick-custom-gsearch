{
  "manifest_version": 2,
  "name": "Quick Custom GSearch",
  "version": "0.2",
  "description": "Quick access for custom search functions",
  "page_action": {
    "default_icon": "19.png"
  },
  "icons" : {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }, 
  "content_scripts": [
    {
      "matches": [
        "*://www.google.co.jp/*" 
      ],
      "css": ["style.css"],
      "js": ["script.js"]
    }
  ]
}
